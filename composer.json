{
    "name": "jadojodo/cloudways-php-sdk",
    "description": "A PHP SDK for interacting with the Cloudways API",
    "type": "library",
    "autoload": {
        "psr-4": {
            "JadoJodo\\CloudwaysPhpSdk\\": "src/"
        }
    },
    "minimum-stability": "alpha",
    "require": {
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "pestphp/pest": "^3.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
